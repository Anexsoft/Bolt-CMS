a:8:{s:5:"token";s:6:"4300af";s:6:"parent";N;s:8:"children";a:0:{}s:4:"data";a:11:{s:6:"config";C:62:"Symfony\Component\HttpKernel\DataCollector\ConfigDataCollector":454:{a:17:{s:8:"app_name";N;s:11:"app_version";N;s:5:"token";s:6:"4300af";s:15:"symfony_version";s:5:"2.8.7";s:13:"symfony_state";s:7:"unknown";s:4:"name";s:3:"n/a";s:3:"env";s:3:"n/a";s:5:"debug";s:3:"n/a";s:11:"php_version";s:6:"5.5.12";s:14:"xdebug_enabled";b:1;s:14:"eaccel_enabled";b:0;s:11:"apc_enabled";b:0;s:14:"xcache_enabled";b:0;s:16:"wincache_enabled";b:0;s:20:"zend_opcache_enabled";b:0;s:7:"bundles";a:0:{}s:9:"sapi_name";s:14:"apache2handler";}}s:7:"request";C:63:"Symfony\Component\HttpKernel\DataCollector\RequestDataCollector":33056:{a:18:{s:6:"format";s:4:"html";s:7:"content";s:13152:"form%5B_token%5D=he9cDS98Ud7rRvg9NLah9YGe0xhtrgVDNjZfiXQnrjc&form%5Bcontents%5D=%23+This+file+defines+the+contenttypes+on+the+website.+See+the+documentation+for%0D%0A%23+details%3A+https%3A%2F%2Fdocs.bolt.cm%2Fmanual%2Fcontenttypes%0D%0A%0D%0A%23+Pages+can+be+used+for+the+more+'static'+pages+on+your+site.+Things+like%0D%0A%23+'about+us'%2C+'contact'+or+a+'disclaimer'.+This+content-type+has+a+'templateselect'%0D%0A%23+field%2C+which+allows+you+to+override+the+record_template+setting+for+an%0D%0A%23+individual+page.%0D%0A%23+The+taxonomy+for+this+contenttype+is+defined+as+'groups'%2C+which+is+a+so-called%0D%0A%23+%22grouping+taxonomy%22.+This+way+you+can+easily+group+sets+of+pages+that+logically%0D%0A%23+belong+together.+If+your+site+has+a+'services'+section%2C+in+which+you'd+like+to%0D%0A%23+list+the+different+types+of+services+a+company+provides%2C+you+can+group+these%0D%0A%23+together.%0D%0A%0D%0Apaginas%3A%0D%0A++++name%3A+paginas%0D%0A++++singular_name%3A+P%C3%A1gina%0D%0A++++fields%3A%0D%0A++++++++title%3A%0D%0A++++++++++++type%3A+text%0D%0A++++++++++++class%3A+large%0D%0A++++++++++++group%3A+content%0D%0A++++++++slug%3A%0D%0A++++++++++++type%3A+slug%0D%0A++++++++++++uses%3A+title%0D%0A++++++++image%3A%0D%